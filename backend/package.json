{
  "name": "backend",
  "version": "0.0.2",
  "type": "module",
  "engines": {
    "node": ">=20.14.0"
  },
  "scripts": {
    "quick": "cross-env PGLITE=true tsx scripts/quick.ts && cross-env PGLITE=true pnpm dev",
    "start": "tsx dist/index.js",
    "dev": "cross-env NODE_ENV=development tsx --watch src/index.ts",
    "check:types": "tsc --pretty",
    "ts:perf": "rm -rf tsconfig.tsbuildinfo && cross-env NODE_ENV=development tsc --noEmit --extendedDiagnostics",
    "build": "cross-env NODE_ENV=production tsup",
    "build:dev": "tsup",
    "docker:up": "docker compose --env-file .env up",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org cella --project backend ./dist && sentry-cli sourcemaps upload --org cella --project backend ./dist",
    "build:sourcemaps": "cross-env NODE_ENV=production tsup && pnpm sentry:sourcemaps",
    "seed:user": "tsx scripts/seeds/user/index.ts",
    "seed:organizations": "tsx scripts/seeds/organizations/index.ts",
    "seed:data": "tsx scripts/seeds/data/index.ts",
    "seed": "tsx scripts/seed.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:clear": "tsx scripts/clear-db.ts",
    "generate:pg": "drizzle-kit generate --config drizzle.config.ts",
    "generate": "pnpm generate:pg",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio --config drizzle.config.ts",
    "emails:build": "email build ./emails",
    "emails:create": "email create",
    "emails:preview": "email preview ./emails",
    "test": "pnpm vitest"
  },
  "dependencies": {
    "@aws-sdk/cloudfront-signer": "^3.734.0",
    "@cellajs/permission-manager": "^0.1.0",
    "@electric-sql/pglite": "^0.2.16",
    "@hono/node-server": "^1.13.8",
    "@hono/sentry": "^1.2.0",
    "@hono/zod-openapi": "0.18.3",
    "@logtail/node": "^0.5.2",
    "@node-rs/argon2": "^2.0.2",
    "@novu/node": "^2.0.6",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation-http": "^0.57.1",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.57.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/webauthn": "^1.0.0",
    "@paddle/paddle-node-sdk": "^2.5.0",
    "@scalar/hono-api-reference": "^0.5.171",
    "@sendgrid/mail": "^8.1.4",
    "@sentry/cli": "^2.41.1",
    "@t3-oss/env-core": "^0.12.0",
    "arctic": "^3.2.2",
    "config": "workspace:*",
    "dompurify": "^3.2.4",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "0.7.0",
    "enforce-unique": "^1.3.0",
    "hono": "4.6.20",
    "i18next": "^24.2.2",
    "isbot": "^5.1.22",
    "jsdom": "^26.0.0",
    "jsonwebtoken": "^9.0.2",
    "jsx-email": "^2.7.0",
    "locales": "workspace:*",
    "nanoid": "^5.0.9",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^5.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "slugify": "^1.6.6",
    "ua-parser-js": "^2.0.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^13.1.0",
    "@faker-js/faker": "^9.4.0",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.1",
    "@types/papaparse": "^5.3.15",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.8",
    "@types/ua-parser-js": "^0.7.39",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.4",
    "jszip": "^3.10.1",
    "papaparse": "^5.5.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "exports": {
    "development": "./src/"
  }
}
