{
  "name": "backend",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.9.0 <21.0.0"
  },
  "scripts": {
    "start": "tsx dist/src/index.cjs",
    "build": "NODE_ENV=production tsup",
    "build:dev": "NODE_ENV=development tsup",
    "check:types": "tsc",
    "dev": "NODE_ENV=development tsup --watch --onSuccess \"tsx dist/src/index.cjs\"",
    "seed:users": "tsx seed/users.ts",
    "seed:organizations": "tsx seed/organizations.ts",
    "generate": "drizzle-kit generate:pg",
    "push": "drizzle-kit push:pg",
    "migrate": "tsx src/db/migrate.ts",
    "migrate:script": "tsx dist/src/db/migrate.cjs",
    "seeds:script": "tsx dist/seed/users.cjs && tsx dist/seed/organizations.cjs",
    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@appsignal/nodejs": "^3.0.30",
    "@cellajs/imado": "^0.1.2",
    "@hono/node-server": "^1.8.2",
    "@hono/swagger-ui": "^0.2.1",
    "@hono/zod-openapi": "0.9.8",
    "@lucia-auth/adapter-drizzle": "^1.0.2",
    "@lucia-auth/adapter-postgresql": "^3.1.0",
    "@react-email/render": "^0.0.12",
    "@types/jsonwebtoken": "^9.0.6",
    "arctic": "^1.2.1",
    "config": "workspace:*",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.4",
    "drizzle-zod": "^0.5.1",
    "email": "workspace:*",
    "enforce-unique": "^1.2.0",
    "env": "workspace:*",
    "hono": "4.0.8",
    "i18next": "^23.10.0",
    "jsonwebtoken": "^9.0.2",
    "locales": "workspace:*",
    "lucia": "^3.0.1",
    "nanoid": "^5.0.4",
    "node-cron": "^3.0.3",
    "oslo": "^1.1.3",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "rate-limiter-flexible": "^5.0.0",
    "react-i18next": "^14.0.5",
    "slugify": "^1.6.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "5.5.0",
    "@babel/preset-typescript": "^7.23.3",
    "@faker-js/faker": "^8.3.1",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.2",
    "drizzle-kit": "^0.20.13",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ]
  },
  "exports": {
    "./*": "./src/*.ts"
  }
}