    "@typescript/native-preview": "7.0.0-dev.20260129.1",
    "commander": "^14.0.3",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "1.0.0-beta.15-859cf75",
    "papaparse": "^5.5.3",
    "picocolors": "^1.1.1",
