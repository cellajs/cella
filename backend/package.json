{
  "name": "@cella/backend",
  "version": "0.0.2",
  "type": "module",
  "engines": {
    "node": ">=24.5.0"
  },
  "scripts": {
    "quick": "cross-env PGLITE=true tsx scripts/quick.ts && cross-env PGLITE=true pnpm dev",
    "start": "cross-env NODE_ENV=production tsx dist/index.js",
    "dev": "cross-env NODE_ENV=development tsx --watch-path=src src/index.ts",
    "tunnel": "cross-env NODE_ENV=tunnel tsx --watch-path=src src/index.ts",
    "ts": "tsc --pretty",
    "ts:beta": "tsgo --pretty",
    "tsperf": "rm -rf dist && tsc --extendedDiagnostics",
    "tsperf:beta": "rm -rf dist && tsgo --extendedDiagnostics",
    "build": "cross-env NODE_ENV=production tsup",
    "build:dev": "tsup",
    "build:staging": "cross-env NODE_ENV=staging tsup",
    "docker:up": "docker compose --env-file .env up",
    "docker:down": "docker compose down",
    "stop": "pnpm docker:down",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org cella --project backend dist && sentry-cli sourcemaps upload --org cella --project backend dist",
    "build:sourcemaps": "cross-env NODE_ENV=production tsup && pnpm sentry:sourcemaps",
    "seed:user": "tsx scripts/seeds/user/run.ts",
    "seed:organizations": "tsx scripts/seeds/organizations/run.ts",
    "seed:data": "tsx scripts/seeds/data/run.ts",
    "seed": "tsx scripts/seed.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:clear": "tsx scripts/clear-db.ts",
    "generate:pg": "drizzle-kit generate --config drizzle.config.ts",
    "generate": "pnpm generate:pg",
    "generate-openapi": "tsx scripts/generate-openapi.ts || true",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio --config drizzle.config.ts",
    "emails:build": "email build emails",
    "emails:create": "email create",
    "emails:preview": "email preview emails",
    "test": "pnpm vitest"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "8.1.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/cloudfront-signer": "^3.893.0",
    "@aws-sdk/lib-storage": "^3.896.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@cellajs/permission-manager": "^0.1.0",
    "@dotenv-run/core": "^1.3.8",
    "@electric-sql/pglite": "^0.3.10",
    "@getbrevo/brevo": "2.5.0",
    "@hono/node-server": "^1.19.4",
    "@hono/zod-openapi": "1.1.3",
    "@logtail/node": "^0.5.6",
    "@node-rs/argon2": "^2.0.2",
    "@novu/api": "^1.6.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/otp": "^1.1.0",
    "@oslojs/webauthn": "^1.0.0",
    "@paddle/paddle-node-sdk": "^3.2.1",
    "@scalar/hono-api-reference": "^0.9.19",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/cli": "^2.55.0",
    "@sentry/node": "^9.46.0",
    "@sentry/profiling-node": "^9.46.0",
    "@t3-oss/env-core": "^0.13.8",
    "arctic": "^3.7.0",
    "config": "workspace:*",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "0.8.3",
    "enforce-unique": "^1.3.0",
    "hono": "4.9.8",
    "i18next": "^25.5.2",
    "isbot": "^5.1.31",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "jsx-email": "^2.7.3",
    "locales": "workspace:*",
    "nanoid": "^5.1.6",
    "node-html-parser": "^7.0.1",
    "pg": "^8.16.3",
    "pino": "^9.11.0",
    "pino-http": "^10.5.0",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^7.4.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "slugify": "^1.6.6",
    "ua-parser-js": "^2.0.5",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@faker-js/faker": "^10.0.0",
    "@ngrok/ngrok": "^1.5.2",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/papaparse": "^5.3.16",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.13",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript/native-preview": "7.0.0-dev.20250910.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4",
    "papaparse": "^5.5.3",
    "pino-pretty": "^13.1.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "exports": {
    "types": "./dist/src/index.d.ts"
  },
  "types": "./dist/src/index.d.ts"
}
