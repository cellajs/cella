{
  "name": "@cella/backend",
  "version": "0.0.2",
  "type": "module",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "quick": "cross-env PGLITE=true tsx scripts/quick.ts && cross-env PGLITE=true pnpm dev",
    "start": "cross-env NODE_ENV=production tsx dist/index.js",
    "dev": "cross-env NODE_ENV=development tsx --watch-path=src src/index.ts",
    "tunnel": "cross-env NODE_ENV=tunnel tsx --watch-path=src src/index.ts",
    "ts": "tsc --pretty",
    "ts:beta": "tsgo --pretty",
    "tsperf": "rm -rf dist && tsc --extendedDiagnostics",
    "tsperf:beta": "rm -rf dist && tsgo --extendedDiagnostics",
    "build": "cross-env NODE_ENV=production tsup",
    "build:dev": "tsup",
    "build:staging": "cross-env NODE_ENV=staging tsup",
    "docker:up": "docker compose --env-file .env up",
    "docker:down": "docker compose down",
    "stop": "pnpm docker:down",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org cella --project backend dist && sentry-cli sourcemaps upload --org cella --project backend dist",
    "build:sourcemaps": "cross-env NODE_ENV=production tsup && pnpm sentry:sourcemaps",
    "seed:user": "tsx scripts/seeds/user/run.ts",
    "seed:organizations": "tsx scripts/seeds/organizations/run.ts",
    "seed:data": "tsx scripts/seeds/data/run.ts",
    "seed:pages": "tsx scripts/seeds/pages.ts",
    "seed": "tsx scripts/seed.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:clear": "tsx scripts/clear-db.ts",
    "generate:pg": "drizzle-kit generate --config drizzle.config.ts",
    "generate": "pnpm generate:pg",
    "generate-openapi": "tsx scripts/generate-openapi.ts || true",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio --config drizzle.config.ts",
    "emails:build": "email build emails",
    "emails:create": "email create",
    "emails:preview": "email preview emails",
    "test": "pnpm vitest"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "@aws-sdk/client-s3": "^3.966.0",
    "@aws-sdk/cloudfront-signer": "^3.966.0",
    "@aws-sdk/lib-storage": "^3.966.0",
    "@aws-sdk/s3-request-presigner": "^3.966.0",
    "@cellajs/permission-manager": "^0.1.0",
    "@dotenv-run/core": "^1.3.8",
    "@electric-sql/pglite": "^0.3.14",
    "@getbrevo/brevo": "2.5.0",
    "@hono/node-server": "^1.19.8",
    "@hono/zod-openapi": "^1.2.0",
    "@logtail/node": "^0.5.6",
    "@node-rs/argon2": "^2.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation-http": "^0.208.0",
    "@opentelemetry/resources": "^2.3.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.3.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/otp": "^1.1.0",
    "@oslojs/webauthn": "^1.0.0",
    "@paddle/paddle-node-sdk": "^3.5.0",
    "@scalar/hono-api-reference": "^0.9.32",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/cli": "^3.0.3",
    "@sentry/node": "^10.32.1",
    "@sentry/profiling-node": "^10.32.1",
    "@t3-oss/env-core": "^0.13.10",
    "arctic": "^3.7.0",
    "config": "workspace:*",
    "drizzle-orm": "^0.45.1",
    "drizzle-seed": "^0.3.1",
    "drizzle-zod": "^0.8.3",
    "enforce-unique": "^1.3.0",
    "hono": "^4.11.3",
    "i18next": "^25.7.4",
    "isbot": "^5.1.32",
    "jsdom": "^27.4.0",
    "jsonwebtoken": "^9.0.3",
    "jsx-email": "^2.8.3",
    "locales": "workspace:*",
    "nanoid": "^5.1.6",
    "node-html-parser": "^7.0.2",
    "pg": "^8.16.3",
    "pino": "^10.1.1",
    "pino-http": "^11.0.0",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^9.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.2",
    "slugify": "^1.6.6",
    "ua-parser-js": "^2.0.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@faker-js/faker": "^10.2.0",
    "@ngrok/ngrok": "^1.7.0",
    "@types/jsdom": "^27.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.7",
    "@types/papaparse": "^5.5.2",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.8",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript/native-preview": "7.0.0-dev.20260101.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "papaparse": "^5.5.3",
    "pino-pretty": "^13.1.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "exports": {
    "types": "./dist/src/index.d.ts"
  },
  "types": "./dist/src/index.d.ts"
}
