{
  "name": "@cella/frontend",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "quick": "cross-env VITE_QUICK=true pnpm dev",
    "dev": "wait-on http://localhost:4000/health && pnpm generate-client && pnpm concurrently --names \"VITE,STORY\" --prefix name \"pnpm dev:run\" \"pnpm storybook\"",
    "dev:run": "cross-env NODE_ENV=development vite --mode development",
    "tunnel": "wait-on http://localhost:4000/health && cross-env NODE_ENV=tunnel vite --mode tunnel",
    "start": "cross-env NODE_ENV=production pnpm preview",
    "test": "vitest run",
    "ts": "tsgo --pretty",
    "ts:old": "tsc --pretty",
    "tsperf": "rm -rf tsconfig.tsbuildinfo && cross-env NODE_ENV=development tsgo --noEmit --extendedDiagnostics",
    "tsperf:old": "rm -rf tsconfig.tsbuildinfo && cross-env NODE_ENV=development tsc --noEmit --extendedDiagnostics",
    "build": "cross-env NODE_ENV=production vite build",
    "build:dev": "cross-env NODE_ENV=development vite build --mode development",
    "build:staging": "cross-env NODE_ENV=staging vite build --mode staging",
    "preview": "vite preview --port 3000",
    "preview:dev": "cross-env NODE_ENV=development vite preview --port 3000",
    "stop": "tsx vite/stop-vite.ts",
    "generate-client": "tsx vite/generate-client.ts",
    "storybook": "cross-env STORYBOOK=true storybook dev -p 6006 --quiet --no-open",
    "build-storybook": "storybook build",
    "fix-exports": "tsx scripts/fix-exports.ts",
    "fix-exports:dry": "tsx scripts/fix-exports.ts --dry-run"
  },
