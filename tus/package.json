{
  "name": "tus",
  "version": "1.0.0",
  "description": "TUS server to handle file uploading with imado",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "build:dev": "NODE_ENV=development tsup",
    "start": "NODE_ENV=production tsx dist/index.cjs",
    "dev": "NODE_ENV=development tsup --watch --onSuccess \"tsx dist/index.cjs\""
  },
  "keywords": ["tus", "imado"],
  "author": "CellaJS <info@cellajs.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "@cellajs/imado": "^0.1.2",
    "config": "workspace:*",
    "env": "workspace:*",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
