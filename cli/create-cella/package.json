{
  "name": "@cellajs/create-cella",
  "version": "0.2.0",
  "private": false,
  "license": "MIT",
  "description": "Cella is a TypeScript template to create web apps with sync and offline capabilities.",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cellajs/cella",
    "directory": "cli/create-cella"
  },
  "homepage": "https://cellajs.com",
  "author": "CellaJS <info@cellajs.com>",
  "engines": {
    "node": "24.x"
  },
  "type": "module",
  "main": "./src/index.ts",
  "imports": {
    "#/*": "./src/*"
  },
  "bin": {
    "create-cella": "index.js"
  },
  "scripts": {
    "start": "tsx ./src/index.ts",
    "clean": "rimraf ./dist",
    "build": "tsup",
    "test-build": "pnpm run build && node index.js",
    "prepublishOnly": "pnpm run build",
    "check": "pnpm ts && pnpm biome check --write .",
    "ts": "tsc --pretty",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "axios": "^1.13.4",
    "commander": "^14.0.3",
    "giget": "^3.1.1",
    "nano-spawn": "^2.0.0",
    "ora": "^9.2.0",
    "picocolors": "^1.1.1",
    "validate-npm-package-name": "^7.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.10",
    "@types/validate-npm-package-name": "^4.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0"
  }
}
